{
    "name": "birthhelper",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.js",
        "start:local": "ng serve --configuration=local",
        "start:remote": "ng serve --configuration=remote",
        "start:localhost": "ng serve",
        "build": "npm run envgen && ng build --prod",
        "docker:build": "npm i && npm run build && docker build -t exclusivelogin/birthhelper-front .",
        "docker:publish": "npm run docker:build && docker login && docker push exclusivelogin/birthhelper-front",
        "docker:run": "docker pull exclusivelogin/birthhelper-front && docker run --rm -d -p 8092:80 exclusivelogin/birthhelper-front",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "envgen": "echo 'environment generating... ' && node envgen.js && echo 'environment generated'"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.2.4",
        "@angular/common": "^11.2.4",
        "@angular/compiler": "^11.2.4",
        "@angular/core": "^11.2.4",
        "@angular/forms": "^11.2.4",
        "@angular/platform-browser": "^11.2.4",
        "@angular/platform-browser-dynamic": "^11.2.4",
        "@angular/router": "^11.2.4",
        "@ngneat/until-destroy": "^8.1.1",
        "@types/moment": "^2.13.0",
        "bootstrap": "^4.6.0",
        "core-js": "^2.4.1",
        "leaflet": "^1.6.0",
        "md5": "^2.3.0",
        "moment": "^2.29.1",
        "ngx-toastr": "^13.2.0",
        "rxjs": "^6.6.6",
        "rxjs-compat": "^6.0.0-rc.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.3",
        "@angular/cli": "~11.2.3",
        "@angular/compiler-cli": "^11.2.4",
        "@angular/language-service": "^11.2.4",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.2",
        "@types/leaflet": "~1.5.17",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "rxjs-tslint-rules": "^4.34.7",
        "ts-node": "~4.1.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.7"
    }
}
